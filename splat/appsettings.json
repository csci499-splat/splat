{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "splat": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "/admin",
        "LogoutUri": "/login"
      }
    }
  },
